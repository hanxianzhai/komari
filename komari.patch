 func AddClient(c *gin.Context) {
 	var req struct {
 		Name string `json:"name"`
+		UUID string `json:"uuid"` // 可选的自定义UUID
 	}
 	if err := c.ShouldBindJSON(&req); err != nil || req.Name == "" {
+		// 如果没有提供正确的JSON或名称为空，创建默认客户端
 		uuid, token, err := clients.CreateClient()
 		if err != nil {
 			c.JSON(http.StatusInternalServerError, gin.H{"status": "error", "message": err.Error()})
@@ -23,7 +25,19 @@
 		c.JSON(http.StatusOK, gin.H{"status": "success", "uuid": uuid, "token": token})
 		return
 	}
-	uuid, token, err := clients.CreateClientWithName(req.Name)
+	
+	var uuid, token string
+	var err error
+	
+	// 检查是否提供了自定义UUID
+	if req.UUID != "" {
+		// 使用自定义UUID创建客户端
+		uuid, token, err = clients.CreateClientWithUUID(req.Name, req.UUID)
+	} else {
+		// 使用默认方法创建客户端
+		uuid, token, err = clients.CreateClientWithName(req.Name)
+	}
+	
 	if err != nil {
 		c.JSON(http.StatusInternalServerError, gin.H{"status": "error", "message": err.Error()})
 		return
