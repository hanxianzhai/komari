+// CreateClientWithUUID 创建指定UUID的客户端
+func CreateClientWithUUID(name, customUUID string) (clientUUID, token string, err error) {
+	// 验证UUID格式
+	if _, err := uuid.Parse(customUUID); err != nil {
+		return "", "", fmt.Errorf("invalid UUID format: %w", err)
+	}
+	
+	// 检查UUID是否已存在
+	db := dbcore.GetDBInstance()
+	var existingClient models.Client
+	if err := db.Where("uuid = ?", customUUID).First(&existingClient).Error; err == nil {
+		// 如果UUID已存在，返回现有客户端的token
+		return existingClient.UUID, existingClient.Token, nil
+	}
+	
+	// 创建新客户端
+	token = utils.GenerateToken()
+	client := models.Client{
+		UUID:      customUUID,
+		Token:     token,
+		Name:      name,
+		CreatedAt: models.FromTime(time.Now()),
+		UpdatedAt: models.FromTime(time.Now()),
+	}
+
+	err = db.Create(&client).Error
+	if err != nil {
+		return "", "", err
+	}
+	return customUUID, token, nil
 }
 
